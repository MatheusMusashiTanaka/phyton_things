i=0"""



tr.pendown()

for i in range(4):
    tr.forward(200)
    tr.left(angulo)

tr.left(45)
tr.forward(150)
tr.left(45)
tr.forward(200)
for i in range(4):
    tr.right(90)
    tr.forward(200)
    
tr.right(90)
tr.forward(200)
tr.right(135)
tr.forward(150)
tr.left(45)
tr.forward(200)

tr.left(135)
tr.forward(150)
tr.left(135)
tr.forward(200)

tr.right(90)
tr.forward(200)
tr.left(135)
tr.forward(150)
